{
  "terminal.integrated.fontSize": 14,
  "security.workspace.trust.untrustedFiles": "open",
  "explorer.confirmDelete": false,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "editor.scrollBeyondLastLine": false,
  // save
  "vim.commandLineModeKeyBindingsNonRecursive": [
    {
      "before": [
        "W",
        "<Enter>"
      ],
      "after": [
        "w",
        "<Enter>"
      ]
    },
    {
      "before": [
        "Q",
        "<Enter>"
      ],
      "after": [
        "q",
        "<Enter>"
      ]
    },
    {
      "before": [
        "Q",
        "!",
        "<Enter>"
      ],
      "after": [
        "q",
        "!",
        "<Enter>"
      ]
    }
  ],
  "vim.normalModeKeyBindingsNonRecursive": [
    // diag
    {
      "before": [
        "]",
        "d"
      ],
      "commands": [
        "editor.action.marker.nextInFiles"
      ],
      "silent": true
    },
    {
      "before": [
        "[",
        "d"
      ],
      "commands": [
        "editor.action.marker.prevInFiles"
      ],
      "silent": true
    },
    // ctrl groups
    {
      "before": ["1", "1"],
      "after": ["<Esc>"],
      "silent": true,
      "commands": [
        {
          "command": "sc2.jumpToControlGroup",
          "args": { "id": 1 }
        }
      ]
    },
    {
      "before": ["2", "2"],
      "after": ["<Esc>"],
      "silent": true,
      "commands": [
        {
          "command": "sc2.jumpToControlGroup",
          "args": { "id": 2 }
        }
      ]
    },
    {
      "before": ["3", "3"],
      "after": ["<Esc>"],
      "silent": true,
      "commands": [
        {
          "command": "sc2.jumpToControlGroup",
          "args": { "id": 3 }
        }
      ]
    },
    {
      "before": ["4", "4"],
      "after": ["<Esc>"],
      "silent": true,
      "commands": [
        {
          "command": "sc2.jumpToControlGroup",
          "args": { "id": 4 }
        }
      ]
    },
    {
      "before": ["5", "5"],
      "after": ["<Esc>"],
      "silent": true,
      "commands": [
        {
          "command": "sc2.jumpToControlGroup",
          "args": { "id": 5 }
        }
      ]
    },
    {
      "before": ["6", "6"],
      "after": ["<Esc>"],
      "silent": true,
      "commands": [
        {
          "command": "sc2.jumpToControlGroup",
          "args": { "id": 6 }
        }
      ]
    },
    {
      "before": ["7", "7"],
      "after": ["<Esc>"],
      "silent": true,
      "commands": [
        {
          "command": "sc2.jumpToControlGroup",
          "args": { "id": 7 }
        }
      ]
    },
    {
      "before": ["8", "8"],
      "after": ["<Esc>"],
      "silent": true,
      "commands": [
        {
          "command": "sc2.jumpToControlGroup",
          "args": { "id": 8 }
        }
      ]
    },
    {
      "before": ["9", "9"],
      "after": ["<Esc>"],
      "silent": true,
      "commands": [
        {
          "command": "sc2.jumpToControlGroup",
          "args": { "id": 9 }
        }
      ]
    }
  ],
  "extensions.experimental.affinity": {
    "vscodevim.vim": 1
  },
  "editor.stopRenderingLineAfter": 500,
  "git.confirmSync": false,
  "explorer.confirmDragAndDrop": false,
  "vim.vimrc.path": "$HOME/.vimrc",
  "javascript.updateImportsOnFileMove.enabled": "always",
  "workbench.tree.indent": 16,
  "go.toolsManagement.autoUpdate": true,
  "workbench.settings.applyToAllProfiles": [
    "editor.tabSize"
  ],
  "redhat.telemetry.enabled": false,
  "terminal.integrated.defaultProfile.windows": "Git Bash",
  "vim.useSystemClipboard": true,
  "files.associations": {
    "TODO": "markdown"
  },
  "terminal.integrated.fontFamily": "Droid Sans Mono, Consolas, monospace",
  "explorer.confirmPasteNative": false,
  "git.ignoreMissingGitWarning": true,
  "git.autofetch": true,
  "git.ignoreRebaseWarning": true,
  "application.shellEnvironmentResolutionTimeout": 1,
  "diffEditor.ignoreTrimWhitespace": false,
  "[typescriptreact]": {
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "[typescript]": {
    "editor.defaultFormatter": "vscode.typescript-language-features",
    "editor.tabSize": 2
  },
  "editor.minimap.enabled": false,
  "[css]": {
    "editor.defaultFormatter": "vscode.css-language-features"
  },
  "[javascript]": {
    "editor.defaultFormatter": "vscode.typescript-language-features",
    "editor.tabSize": 2
  },
  "settingsSync.ignoredExtensions": [
    "wolfieshorizon.python-auto-venv",
    "equinusocio.vsc-material-theme"
  ],
  "vim.vimrc.enable": true,
  "python.REPL.enableREPLSmartSend": false,
  "interactiveWindow.executeWithShiftEnter": false,
  "workbench.editor.enablePreview": false,
  "[markdown]": {
    "diffEditor.ignoreTrimWhitespace": true
  },
  "terminal.integrated.gpuAcceleration": "off",
  "window.menuBarVisibility": "compact",
  "[jsonc]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "editor.tabSize": 2,
  "vim.sneak": true,
  "vim.sneakReplacesF": true,
  "vim.targets.enable": true,
  "cmake.showOptionsMovedNotification": false,
  "debug.onTaskErrors": "debugAnyway",
  "editor.fontSize": 16,
  "editor.mouseWheelScrollSensitivity": 0.75,
  "chat.commandCenter.enabled": false
}