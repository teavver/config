{
  "terminal.integrated.fontSize": 13,
  "security.workspace.trust.untrustedFiles": "open",
  "explorer.confirmDelete": false,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "editor.scrollBeyondLastLine": false,
  // save
  "vim.commandLineModeKeyBindingsNonRecursive": [
    {
      "before": [
        "W",
        "<Enter>"
      ],
      "after": [
        "w",
        "<Enter>"
      ],
      "silent": true
    },
  ],
  // diag
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": [
        "]",
        "d"
      ],
      "commands": [
        "editor.action.marker.nextInFiles"
      ],
      "silent": true
    },
    {
      "before": [
        "[",
        "d"
      ],
      "commands": [
        "editor.action.marker.prevInFiles"
      ],
      "silent": true
    }
  ],
  "tailwindCSS.experimental.classRegex": [],
  "remote.SSH.remotePlatform": {
    "ssh0.tumer.pl": "linux"
  },
  "editor.stopRenderingLineAfter": 500,
  "git.confirmSync": false,
  "explorer.confirmDragAndDrop": false,
  "vim.vimrc.path": "$HOME/.vimrc",
  "liveServer.settings.donotShowInfoMsg": true,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "solidity.compileUsingRemoteVersion": "v0.8.24",
  "workbench.tree.indent": 24,
  "go.toolsManagement.autoUpdate": true,
  "vim.insertModeKeyBindings": [],
  "vim.normalModeKeyBindings": [],
  "workbench.settings.applyToAllProfiles": [
    "editor.tabSize"
  ],
  "editor.detectIndentation": false,
  "redhat.telemetry.enabled": false,
  "terminal.integrated.defaultProfile.windows": "Git Bash",
  "editor.renderWhitespace": "all",
  "cmake.configureOnOpen": true,
  "vim.useSystemClipboard": true,
  "files.associations": {
    "TODO": "markdown"
  },
  "terminal.integrated.fontFamily": "Droid Sans Mono, Consolas, monospace",
  "explorer.confirmPasteNative": false,
  "git.ignoreMissingGitWarning": true,
  "git.autofetch": true,
  "solidity.solhintRules": {},
  "solidity.telemetry": false,
  "git.ignoreRebaseWarning": true,
  "application.shellEnvironmentResolutionTimeout": 1,
  "diffEditor.ignoreTrimWhitespace": false,
  "[typescriptreact]": {
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "[typescript]": {
    "editor.defaultFormatter": "vscode.typescript-language-features",
    "editor.tabSize": 2
  },
  "editor.minimap.enabled": false,
  "[css]": {
    "editor.defaultFormatter": "vscode.css-language-features"
  },
  "[javascript]": {
    "editor.defaultFormatter": "vscode.typescript-language-features",
    "editor.tabSize": 2
  },
  "settingsSync.ignoredExtensions": [
    "wolfieshorizon.python-auto-venv"
  ],
  "vim.vimrc.enable": true,
  "python.REPL.enableREPLSmartSend": false,
  "interactiveWindow.executeWithShiftEnter": false,
  "workbench.editor.enablePreview": false,
  "[markdown]": {
    "diffEditor.ignoreTrimWhitespace": true
  },
  "editor.fontFamily": "'Menlo', 'monospace', monospace",
  "terminal.integrated.gpuAcceleration": "off",
  "window.menuBarVisibility": "compact",
  "[jsonc]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "github.copilot.advanced": {
    "authProvider": "github"
  },
  "editor.tabSize": 2
}